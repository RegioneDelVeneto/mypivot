			<xsd:complexType name="pivotSILChiediPagatiRiconciliatiRisposta">
				<xsd:complexContent>
					<xsd:extension base="ppt:risposta">
						<xsd:sequence>
							<xsd:element name="pagamentiRiconciliatiPerTipoDovuto" type="ppt:ctPagamentiRiconciliatiPerTipoDovuto" minOccurs="0" maxOccurs="1000" />
