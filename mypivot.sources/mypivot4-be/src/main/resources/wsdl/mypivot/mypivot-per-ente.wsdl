					<xsd:element name="dataOraMessaggioRicevuta" type="pay_i_rve:stISODateTime" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Indica la data del messaggio di quietanza,
								secondo il formato ISO 8601
							</xsd:documentation>
							<xsd:documentation>[YYYY]-[MM]-[DD]T[hh]:[mm]:[ss]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="riferimentoMessaggioRichiesta" type="pay_i_rve:stText35" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Con riferimento al messaggio di Esito
								l’elemento contiene il dato identificativoMessaggioRichiesta
								legato alla trasmissione della
								Richiesta di Pagamento (RP).
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="riferimentoDataRichiesta" type="pay_i_rve:stISODate" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Indica la data secondo il formato ISO 8601
								[YYYY]-[MM]-[DD] cui si riferisce la generazione del dato
