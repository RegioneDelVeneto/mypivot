			<xsd:complexType name="ctExport">
				<xsd:sequence>
					<xsd:element name="versioneOggetto" type="pay_i_rve:stText16" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Riporta la stessa informazione presente nel
								dato “versioneOggetto” della Richiesta di Pagamento (RP).
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="dominio" type="pay_i_rve:ctDominio" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Riporta le stesse informazioni presenti nel
								blocco “Dominio” della Richiesta di Pagamento (RP)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="identificativoMessaggioRicevuta" type="pay_i_rve:stText35" minOccurs="1">
